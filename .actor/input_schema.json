{
    "title": "Climbing information scraper input schema",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "areaQuery": {
            "title": "Area query",
            "type": "string",
            "description": "Area to crawl for crags and routes",
            "editor": "textfield",
            "example": "Czechia",
            "prefill": "Czechia"
        },
        "routeStyle": {
            "sectionCaption": "Optional route filters section",
            "sectionDescription": "In this section, you are able to select filters for the routes",
            "title": "Route style",
            "type": "string",
            "description": "Select the route style",
            "editor": "select",
            "enum": ["", "boulder", "trad", "sport"],
            "enumTitles": ["Empty", "Boulder", "Trad", "Sport"],
            "default": ""
        },
        "routeSetting": {
            "title": "Route setting",
            "type": "string",
            "description": "Select the route setting",
            "editor": "select",
            "enum": ["", "natural", "artificial"],
            "enumTitles": ["Empty", "Natural", "Artificial"],
            "default": ""
        },
        "routeLength": {
            "title": "Height / length",
            "type": "string",
            "description": "Route height/length",
            "editor": "select",
            "enum": ["", "0+5", "5+10", "10+20", "20+45", "45+100", "100+1000"],
            "enumTitles": ["Empty", "0 to 5m", "5 to 10m", "10 to 20m", "20 to 45m", "45 to 100m", "100m to 1km"],
            "default": ""
        },
        "gradingSystem": {
            "sectionCaption": "Route grade information",
            "sectionDescription": "In this section, you are able to select the grading system and min/max route difficulties, if you select the French one, min and max route difficulties should be entered in format: {number}{letter}{?+}, example: 6a+, the range of grades can be from 1a to 9c. If you select the UIAA option, the input format is: {number}{?-/+}, example: 7-, the range is from 1- to 12+",
            "title": "Grading system",
            "type": "string",
            "description": "Route grading system",
            "editor": "select",
            "enum": ["", "FR", "UIAA"],
            "enumTitles": ["Empty", "French", "UIAA"],
            "default": ""
        },
        "minDifficulty": {
            "title": "Min. route difficulty",
            "type": "string",
            "editor": "textfield",
            "description": "Minimal route difficulty",
            "example": "5- / 5b"
        },
        "maxDifficulty": {
            "title": "Max. route difficulty",
            "type": "string",
            "editor": "textfield",
            "description": "Maximal route difficulty",
            "example": "6+ / 6c"
        },
        "maxRequestsPerCrawl": {
            "title": "Max Requests per Crawl",
            "type": "integer",
            "description": "Maximum number of requests that can be made by this crawler.",
            "default": 100
        },
        "aiMode": {
            "sectionCaption": "AI Section",
            "sectionDescription": "AI Section description",
            "title": "Google AI Mode",
            "description": "This section is responsible for Google AI Mode settings",
            "type": "object",
            "editor": "schemaBased",
            "default": {
                "aiMode": "aiModeOff"
            },
            "properties": {
                "aiMode": {
                    "title": "Google AI Mode",
                    "description": "Enable scraping of Google's AI Mode to perform Answer Engine Optimization (AEO), GEO targeting, track brand visibility, and analyze competitors.",
                    "enum": [
                        "aiModeOff",
                        "aiModeWithSearchResults",
                        "aiModeOnly"
                    ],
                    "enumTitles": [
                        "AI Mode off",
                        "AI Mode + Search Results",
                        "AI Mode only"
                    ],
                    "type": "string",
                    "editor": "select"                
                }
            }
        },
        "perplexitySearch": {
            "title": "Perplexity AI search",
            "description": "Enable Perplexity to retrieve AI-generated answers and citations using the Sonar model. This feature is designed for cross-platform analysis, allowing you to directly compare Google AI Mode results against Perplexityâ€™s perspective to identify narrative differences and coverage gaps. Note: An additional fee applies per result when this feature is active. Please refer to the Pricing tab for your specific rate.",
            "type": "object",
            "editor": "schemaBased",
            "default": {
                "enablePerplexity": false
            },
            "prefill": {
                "enablePerplexity": false
            },
            "properties": {
                "enablePerplexity": {
                    "title": "Enable Perplexity AI",
                    "description": "Fetches an AI answer from Perplexity for every query to compare against Google.",
                    "type": "boolean"
                }
            }
        },
        "subschemaObject2": {
            "sectionCaption": "Trying this in section",
            "sectionDescription": "This is section description",
            "title": "Object with sub-schema",
            "type": "object",
            "description": "This is object field with sub-schema defined and schemaBased editor so each sub-field is rendered",
            "editor": "schemaBased",
            "default": {
                "key1": "default value"
            },
            "prefill": {
                "key1": "Prefill value"
            },
            "properties": {
                "key1": {
                    "title": "Key1 string sub-property",
                    "type": "string",
                    "description": "This is string sub-property with textfield editor",
                    "editor": "textfield"
                },
                "key2": {
                    "title": "Key2 integer sub-property",
                    "type": "integer",
                    "description": "This is integer sub-property with number editor",
                    "editor": "number"
                },
                "key3": {
                    "title": "Key3 object sub-property",
                    "type": "object",
                    "description": "This is object sub-property with json editor. It has also sub-schema defined",
                    "editor": "json",
                    "properties": {
                        "key3-1": {
                            "title": "key1 sub-property",
                            "type": "string",
                            "description": "This is sub-property"
                        },
                        "key3-2": {
                            "title": "key2 sub-property",
                            "type": "string",
                            "description": "This is sub-property"
                        }
                    }
                },
                "key.with.dot": {
                    "title": "Key.with.dot object sub-property",
                    "type": "object",
                    "description": "This is another object sub-property but it has dot in it's key.",
                    "editor": "json",
                    "properties": {
                        "key-1": {
                            "title": "key1 sub-property",
                            "type": "string",
                            "description": "This is sub-property"
                        },
                        "key-2": {
                            "title": "key2 sub-property",
                            "type": "string",
                            "description": "This is sub-property"
                        }
                    },
                    "required": [
                        "key3-2"
                    ],
                    "additionalProperties": false
                }
            },
            "required": [
                "key1"
            ],
            "additionalProperties": true
        },
        "standardarray2": {
            "title": "Key value",
            "type": "array",
            "description": "URLs to start with",
            "editor": "keyValue"
        }
    },
    "required": ["areaQuery"]
}
