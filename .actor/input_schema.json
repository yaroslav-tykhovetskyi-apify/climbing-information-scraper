{
    "title": "Climbing information scraper input schema",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "areaQuery": {
            "title": "Area query",
            "type": "string",
            "description": "Area to crawl for crags and routes",
            "editor": "textfield",
            "example": "Czechia",
            "prefill": "Czechia"
        },
        "routeStyle": {
            "sectionCaption": "Optional route filters section",
            "sectionDescription": "In this section, you are able to select filters for the routes",
            "title": "Route style",
            "type": "string",
            "description": "Select the route style",
            "editor": "select",
            "enum": ["", "boulder", "trad", "sport"],
            "enumTitles": ["Empty", "Boulder", "Trad", "Sport"],
            "default": ""
        },
        "routeSetting": {
            "title": "Route setting",
            "type": "string",
            "description": "Select the route setting",
            "editor": "select",
            "enum": ["", "natural", "artificial"],
            "enumTitles": ["Empty", "Natural", "Artificial"],
            "default": ""
        },
        "routeLength": {
            "title": "Height / length",
            "type": "string",
            "description": "Route height/length",
            "editor": "select",
            "enum": ["", "0+5", "5+10", "10+20", "20+45", "45+100", "100+1000"],
            "enumTitles": ["Empty", "0 to 5m", "5 to 10m", "10 to 20m", "20 to 45m", "45 to 100m", "100m to 1km"],
            "default": ""
        },
        "gradingSystem": {
            "sectionCaption": "Route grade information",
            "sectionDescription": "In this section, you are able to select the grading system and min/max route difficulties, if you select the French one, min and max route difficulties should be entered in format: {number}{letter}{?+}, example: 6a+, the range of grades can be from 1a to 9c. If you select the UIAA option, the input format is: {number}{?-/+}, example: 7-, the range is from 1- to 12+",
            "title": "Grading system",
            "type": "string",
            "description": "Route grading system",
            "editor": "select",
            "enum": ["", "FR", "UIAA"],
            "enumTitles": ["Empty", "French", "UIAA"],
            "default": ""
        },
        "minDifficulty": {
            "title": "Min. route difficulty",
            "type": "string",
            "editor": "textfield",
            "description": "Minimal route difficulty",
            "example": "5- / 5b"
        },
        "maxDifficulty": {
            "title": "Max. route difficulty",
            "type": "string",
            "editor": "textfield",
            "description": "Maximal route difficulty",
            "example": "6+ / 6c"
        },
        "maxRequestsPerCrawl": {
            "title": "Max Requests per Crawl",
            "type": "integer",
            "description": "Maximum number of requests that can be made by this crawler.",
            "default": 100
        },
        "testThing": {
            "sectionCaption": "Test caption",
            "sectionDescription": "Test section description",
            "title": "Test title",
            "type": "object",
            "description": "Test description",
            "editor": "schemaBased",
            "default": {
                "first": false,
                "second": false
            },
            "prefill": {
                "first": false,
                "second": false
            },
            "properties": {
                "first": {
                    "title": "Enable First",
                    "type": "string",
                    "description": "Enable first option",
                    "editor": "select",
                    "enum": [
                        "aiModeOff",
                        "aiModeWithSearchResults",
                        "aiModeOnly"
                    ],
                    "enumTitles": [
                        "AI Mode off",
                        "AI Mode + Search Results",
                        "AI Mode only"
                    ],
                    "default": "aiModeOff"
                },
                "second": {
                    "title": "Second Option",
                    "type": "boolean",
                    "description": "Enable second option"
                }
            }
        }
    },
    "required": ["areaQuery"]
}
